<?xml version="1.0" encoding="utf-8"?>
<templates id="import_dashboard_templates">
    <!--    Dashboard view -->
    <t t-name="imp_dash">
        <section class="import_dashboard_main_section"
                 id="main_section_import_dashboard">
            <div class="main-container">
                <div class="start_msg"
                     style="text-align: center;margin-top:300px;margin-top:300px">
                    <p class="o_view_nocontent_smiling_face">
                        Empty Dashboard
                    </p>
                    <p>
                        Enable Settings To View Your Dashboard.
                    </p>
                </div>
                <div class="row">
                    <!--Sale_order -->
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_sale">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon">
                                        <i class="fa fa-line-chart"
                                           style="font-size:48px;color:purple"/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Sale</h2>
                                            <h6>
                                                <i>Import Sale Order</i>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_sale"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>

                        </div>
                    </div>

                    <!--Purchase_order-->
                    <div class="col-md-3 card3 col-sm-6  card6 col-12 card12"
                         id="card_purchase">
                        <div class="card-shadow" style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon">
                                        <i class="fa fa-credit-card"
                                           style="font-size:48px;color:orange"/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">Purchase</div>
                                        <h6>
                                            <i>Import Purchase Order</i>
                                        </h6>
                                    </div>
                                </div>
                                <button class="btn import_purchase"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <!--Invoice-->
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_invoice">
                        <div class="card-shadow" style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon">
                                        <img src="import_dashboard/static/images/icons8-invoice-64 .png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">Invoice</div>
                                        <h6>
                                            <i>Import Invoice</i>
                                        </h6>

                                    </div>
                                </div>
                                <button class="btn import_invoice"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <!--Partner                -->
                    <div class="col-md-3  card3 col-sm-6  card6 col-12 card12"
                         id="card_partner">
                        <div class="card-shadow" style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/icons8-partner-64.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">Partner
                                        </div>
                                        <h6>
                                            <i>Import Partner</i>
                                        </h6>
                                    </div>
                                </div>
                                <button class="btn import_partner"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>
                    <!--Product_pricelist-->
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_pricelist">
                        <div class="card-shadow" style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/icons8-list-48.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">Product
                                            Pricelist
                                        </div>
                                        <h6>
                                            <i>Import Product Pricelist</i>
                                        </h6>
                                    </div>
                                </div>
                                <button class="btn import_product_pricelist"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_bill_of_material">
                        <div class="card-shadow " style="width: 410px;">
                            <!--                        <div class="oh-card-body">-->
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/icons8-paid-bill-64.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Bill of Material</h2>
                                            <h6>
                                                <i>Import Bill of Material</i>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_bill_of_material"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_pos">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/payment-terminal.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>POS</h2>
                                            <h6>
                                                <i>Import POS Orders</i>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_pos"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3 card3 col-sm-6  card6 col-12 card12"
                         id="card_attendance">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/calendar.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Attendance</h2>
                                            <h6>
                                                <i>Import Attendance</i>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_attendance "
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_payment">
                        <div class="card-shadow " style="width: 410px;">
                            <!--                        <div class="oh-card-body">-->
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/payment.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Payment</h2>
                                            <h6>
                                                <i>Import Payment</i>
                                            </h6>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_payment"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-3  card3 col-sm-6 card6 col-12 card12"
                         id="card_task">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="card-shadow-container">
                                        <div class="stat-icon">
                                            <img src="import_dashboard/static/images/tasks.png"
                                                 width="50"
                                                 height="50" alt=""/>
                                        </div>

                                        <div class="stat-content">
                                            <div class="stat-head">
                                                <h2>Task</h2>
                                                <h6>
                                                    <i>Import Task</i>
                                                </h6>

                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_task"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <!--            Product Template -->
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_product">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/cart.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>

                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Product Template</h2>
                                            <h6>
                                                <i>Import Product Template</i>
                                            </h6>

                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_product_template"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>

                    <!--            Vendor Pricelist -->
                    <div class="col-md-3 card3 col-sm-6 card6 col-12 card12"
                         id="card_vendor_pricelist">
                        <div class="card-shadow " style="width: 410px;">
                            <div class="stat-widget-one" style="display: flex;">
                                <div class="card-shadow-container">
                                    <div class="stat-icon"
                                    >
                                        <img src="import_dashboard/static/images/icons8-vendor-64.png"
                                             width="50"
                                             height="50" alt=""/>
                                    </div>
                                    <div class="stat-content">
                                        <div class="stat-head">
                                            <h2>Vendor Pricelist</h2>
                                            <h6>
                                                <i>Import Vendor Pricelist</i>
                                            </h6>

                                        </div>
                                    </div>
                                </div>
                                <button class="btn import_vendor_pricelist"
                                        style="background:none; border:none;"
                                        type="object"
                                        string="Bill of Material">
                                    <i class="fa fa-cloud-upload"
                                       style="font-size:48px;color:#71639e"/>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    </t>
</templates>
