<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--    snippet inherit for adding  new snippets-->
    <template id="theme_levelup_banner_snippet" inherit_id="website.snippets" name="Coffee banner">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]" position="inside">
            <t t-snippet="theme_levelup.awards" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/Award.png"/>
            <t t-snippet="theme_levelup.service" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/service.png"/>
            <t t-snippet="theme_levelup.latest_features" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/feature.png"/>
            <t t-snippet="theme_levelup.excited" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/excited.png"/>
            <t t-snippet="theme_levelup.client" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/client.png"/>
            <t t-snippet="theme_levelup.client_testimonial" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/testimonial.png"/>
            <t t-snippet="theme_levelup.banner" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/Banner.png"/>
            <t t-snippet="theme_levelup.index_about" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/About.png"/>
            <t t-snippet="theme_levelup.gallery" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/gallery.png"/>
            <t t-snippet="theme_levelup.latest_blog" t-thumbnail="/theme_levelup/static/src/images/snippet.icon/Blog.png"/>
        </xpath>
    </template>
</odoo>