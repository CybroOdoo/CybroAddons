<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
<!--    Risk Type Tree View -->
    <record id="risk_type_view_tree" model="ir.ui.view">
        <field name="name">risk.type.view.tree</field>
        <field name="model">risk.type</field>
        <field name="arch" type="xml">
            <tree string="Risk Type" editable="top">
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>
</odoo>