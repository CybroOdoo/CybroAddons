<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--Franchise Management Menu items-->
    <menuitem id="franchise_dealer_menu_root" name="Franchise"
              web_icon="franchise_management,static/description/icon.png">
         <menuitem id="franchise_registration_menu" name="Registration"
                   sequence="1">
             <menuitem id="registration_model_menu_action"
                       name="All Registrations"
                       action="franchise_dealer_model_action"/>
             <menuitem id="dealer_model_menu_action" name="All Dealers"
                       action="franchise_dealer_action"/>
         </menuitem>
        <menuitem id="franchise_agreement_menu_action" name="Agreement" sequence="2">
             <menuitem id="franchise_agreement_model_menu_action"
                       name="Franchise Agreement"
                       action="franchise_agreement_action"/>
         </menuitem>
        <menuitem id="franchise_report_menu_action" name="Reports" sequence="5">
            <menuitem id="franchise_dealer_report_wizard_action"  name="Dealer Report"
                      action="dealer_report_action"/>
            <menuitem id="franchise_sales_report_wizard_action"
                      name="Sales Report"
                      action="dealer_sale_report_action"/>
        </menuitem>
        <menuitem id="franchise_sales_menu_action"
                  name="Franchise Feedback" sequence="3">
             <menuitem id="dealer_sale_model_menu_action"
                       name="Sales Feedback"
                       action="dealer_sale_action"/>
         </menuitem>
    </menuitem>
</odoo>
